{
    "name": "template",
    "version": "1.0.0",
    "dependencies": {
        "tmodjs": "1.0.1-rc9"
    },
    "tmodjs-config": {
        "output": "../js/tpl",
        "charset": "utf-8",
        "syntax": "simple",
        "helpers": "../js/tpl/helper.js",
        "escape": true,
        "compress": true,
        "type": "cmd",
        "runtime": "templates.js",
        "alias": null,
        "minify": false,
        "cache": true
    }
}